package com.kh.serviceCenter.model.service;
import static com.kh.common.JDBCTemplate.*;

import java.sql.Connection;
import java.util.ArrayList;

import com.kh.common.PageInfo;
import com.kh.serviceCenter.model.dao.ScDao;
import com.kh.serviceCenter.model.vo.ServiceCenter;
public class ScService {

	public int selectFAQList() {
		Connection conn = getConnection();
		
		int listCount = new ScDao().selectFAQList(conn);
		
		close(conn);
		return listCount;
	}

	public ArrayList<ServiceCenter> selectFAQList(PageInfo pi) {
		Connection conn = getConnection();
		
		ArrayList<ServiceCenter> ScList = new ScDao().selectFAQList(conn);
		
		close(conn);
		return listCount;
	}

}
