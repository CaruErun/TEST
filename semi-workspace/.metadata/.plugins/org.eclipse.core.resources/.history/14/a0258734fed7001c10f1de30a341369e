<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
<entry key="selectReport">
	SELECT COUNT(*) "COUNT"
	FROM REPORT
</entry>
<entry key="selectRepList">
	SELECT REP_NO, REP_ENTERDATE, REP_TITLE, REP_CONTENT, REP_USER_ID, REP_REPOTER, REP_STATUS
	FROM REPORT
	WHERE REP_NO BETWEEN ? AND ?
</entry>
<entry key="selectMember">
	SELECT USER_NO,USER_ID,USER_NAME,USER_NNAME,USER_BIRTH,GENDER,EMAIL,PHONE,ADDRESS,ENTERDATE,U_STATUS,USER_RPC
	FROM MEMBER
	WHERE USER_NO = ?
</entry>
</properties>